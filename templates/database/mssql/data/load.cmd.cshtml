set SQLCMD="sqlcmd -S localhost -d @Model.Name -E"

@foreach( string sourceFile in Model.sourceFiles) {

if (sourceFile == Model.Name + ".database.create.generated.sql") continue;
if (sourceFile == "load.cmd") continue;
if (sourceFile == "load.sh") continue;

    @("%SQLCMD% -i ./" + sourceFile + "\n")

}