            $($domainObj) $($domainVar) = new $($domainObj)();

            $(${columns} | ForEach {
@"    
                ${$_.column_name} = Get-Random${$_.column_name}()
"@
            })

            
            $($domainObj).insert($($domainVar));

            last$($domainObj).Push($($domainVar));
            map($domainObj).Add($($domainVar).id, $($domainVar) );
        }

       
