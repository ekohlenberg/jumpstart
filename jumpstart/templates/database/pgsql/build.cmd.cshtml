set PSQLCMD="psql --host=localhost --port=5432 --dbname=@Model.Name --username=postgres"
psql --host=localhost --port=5432 --dbname=postgres --username=postgres --file=./@(Model.Name + ".database.create.generated.sql")

    @foreach( string sourceFile in Model.sourceFiles) {
    
    if (sourceFile != Model.Name + ".database.create.generated.sql") {
        @("$PSQLCMD --file=./" + sourceFile + "\n")
        }
    }